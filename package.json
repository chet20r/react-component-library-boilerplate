{
  "name": "web-uikit",
  "version": "1.0.0",
  "description": "Web ui kit component library",
  "author": "chet20r",
  "license": "MIT",
  "repository": "chet20r/web-uikit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "lint:css": "stylelint './src/**/*.jsx'",
    "eslint": "eslint ./src/**/*.js ./src/**/*.jsx",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
